@{
    Layout = null;
}

<div class="row">
    <div style="text-align: center" class="col-lg-12 table-bordered">March, 2017</div>
</div>
<div class="row">
    <div style="text-align: center" class="col-lg-12 table-bordered"
         ng-bind-template="{{getWeek().firstDay}}/{{getWeek().month1}} - {{getWeek().lastDay}}/{{getWeek().month2}},
            {{getYear()}}">
    </div>
</div>

<div class="row">
    <div class="col-lg-1 table-bordered">GMT+2</div>
    <div class="col-lg-1 table-bordered" ng-repeat="d in generateHeadersForGridWeek()">
        {{d}}
    </div>
</div>

<div class="row" ng-repeat="h in hours">

    <div class="col-lg-1 table-bordered">{{h}}<br><br></div>
    <div class="col-lg-1 table-bordered"><br><br></div>
    <div class="col-lg-1 table-bordered"><br><br></div>
    <div class="col-lg-1 table-bordered"><br><br></div>
    <div class="col-lg-1 table-bordered"><br><br></div>
    <div class="col-lg-1 table-bordered"><br><br></div>
    <div class="col-lg-1 table-bordered"><br><br></div>
    <div class="col-lg-1 table-bordered"><br><br></div>

</div>

